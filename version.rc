#include "include\version.h"
#include <winres.h>

1 VERSIONINFO
 FILEVERSION FLTM_VERSION_MAJOR, FLTM_VERSION_MINOR, FLTM_VERSION_PATCH, FLTM_VERSION_BETA
 PRODUCTVERSION FLTM_VERSION_MAJOR, FLTM_VERSION_MINOR, FLTM_VERSION_PATCH, FLTM_VERSION_BETA
