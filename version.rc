            VALUE "InternalName", "FlatMapMarkersSSE"
            VALUE "LegalCopyright", "MIT License"
            VALUE "ProductName", "FlatMapMarkersSSE"
			VALUE "ProductVersion", FLTM_VERSION_VERSTRING
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
